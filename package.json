{
  "name": "react-express-ssr",
  "version": "1.0.0",
  "description": "React with Server Side Rendering using a Node & Express server",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build-client": "webpack --mode production --config webpack.client.js",
    "build-server": "webpack --mode production --config webpack.server.js",
    "build": "npm run build-client && npm run build-server",
    "start": "npm run clean && npm run build && node dist/server.js",
    "dev": "npm run clean && npm run dev:build && npm-run-all --parallel dev:*",
    "dev:build": "webpack --config webpack.server.js && webpack --config webpack.client.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch dist --exec node dist/server.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.12.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.0",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "isomorphic-style-loader": "^5.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "serialize-javascript": "^4.0.0"
  }
}
